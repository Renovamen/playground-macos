{
  "name": "playground-macos",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js ./",
    "lint-fix": "eslint --fix --ext .js ./"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "eslint --fix --ext .js ./"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "trailingComma": "none"
        }
      ]
    },
    "ignorePatterns": [
      "node_modules/",
      "build/"
    ]
  },
  "dependencies": {
    "@craco/craco": "^6.1.2",
    "@rooks/use-raf": "^4.11.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "date-fns": "2.16.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "framer-motion": "^4.1.11",
    "nightwind": "^1.1.11",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-markdown": "^6.0.1",
    "react-rangeslider": "2.2.0",
    "react-redux": "^7.2.4",
    "react-rnd": "^10.2.4",
    "react-scripts": "4.0.3",
    "react-syntax-highlighter": "^15.4.3",
    "react-webcam": "5.2.2",
    "redux": "^4.1.0",
    "remark-gfm": "^1.0.0",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^9",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "postcss": "^7",
    "sort-package-json": "^1.49.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "4.2.4"
  }
}
