{
  "name": "playground-macos",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "craco build",
    "dev": "DISABLE_ESLINT_PLUGIN=true craco start",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.ts,.tsx ./",
    "test": "craco test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "eslint --ext .js,.ts,.tsx ./"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@craco/craco": "^6.4.2",
    "@rooks/use-raf": "^4.11.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.11",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "date-fns": "2.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "framer-motion": "^4.1.17",
    "nightwind": "^1.1.12",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-markdown": "^7.1.1",
    "react-rangeslider": "2.2.0",
    "react-redux": "^7.2.6",
    "react-rnd": "^10.3.5",
    "react-scripts": "4.0.3",
    "react-syntax-highlighter": "^15.4.5",
    "react-webcam": "6.0.0",
    "redux": "^4.1.2",
    "remark-gfm": "^3.0.1",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^9",
    "husky": "^4.3.8",
    "lint-staged": "^12.1.2",
    "postcss": "^7",
    "sort-package-json": "^1.53.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "^4.5.2"
  }
}
